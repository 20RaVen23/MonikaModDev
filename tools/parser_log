# natsuki scare                                                                                                          | ST 1 | 0-0 | None ~ None ~ None
label natsuki_name_scare(playing_okayev=False):                                                                          | ST 1 | 1-0 | None ~ None ~ None
    if persistent._mas_sensitive_mode:                                                                                   | ST 1 | 0-0 | None ~ None ~ None
        return                                                                                                           | ST 1 | 0-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 0-0 | None ~ None ~ None
    # disable stuff                                                                                                      | ST 1 | 0-0 | None ~ None ~ None
    $ HKBHideButtons()                                                                                                   | ST 1 | 1-0 | None ~ None ~ None
    $ disable_esc()                                                                                                      | ST 1 | 1-0 | None ~ None ~ None
    $ store.songs.enabled = False                                                                                        | ST 1 | 1-0 | None ~ None ~ None
    $ quick_menu = False                                                                                                 | ST 1 | 1-0 | None ~ None ~ None
    $ scary_t5c = "bgm/5_ghost.ogg"                                                                                      | ST 1 | 1-0 | None ~ None ~ None
    $ curr_vol = store.songs.getVolume("music")                                                                          | ST 1 | 1-0 | None ~ None ~ None
    $ renpy.music.set_volume(1.0, channel="music")                                                                       | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    # music adjustment                                                                                                   | ST 1 | 0-0 | None ~ None ~ None
    if playing_okayev:                                                                                                   | ST 1 | 1-0 | None ~ None ~ None
        $ currentpos = get_pos(channel="music")                                                                          | ST 1 | 1-0 | None ~ None ~ None
        $ adjusted_t5c = "<from " + str(currentpos) + " loop 4.444>" + scary_t5c                                         | ST 1 | 1-0 | None ~ None ~ None
        stop music fadeout 2.0                                                                                           | ST 1 | 1-0 | None ~ None ~ None
        $ renpy.music.play(adjusted_t5c, fadein=2.0, tight=True)                                                         | ST 1 | 1-0 | None ~ None ~ None
    else:                                                                                                                | ST 1 | 1-0 | None ~ None ~ None
        stop music                                                                                                       | ST 1 | 1-0 | None ~ None ~ None
        $ play_song("<from 11>" + scary_t5c)                                                                             | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    # play with me scene setup                                                                                           | ST 1 | 0-0 | None ~ None ~ None
    scene black                                                                                                          | ST 1 | 1-0 | None ~ None ~ None
    show darkred zorder 5                                                                                                | ST 1 | 1-0 | None ~ None ~ None
    show natsuki ghost1 zorder 2 at t11                                                                                  | ST 1 | 1-0 | None ~ None ~ None
    show n_rects_ghost1_instant zorder 4                                                                                 | ST 1 | 1-0 | None ~ None ~ None
    show n_rects_ghost2_instant zorder 4                                                                                 | ST 1 | 1-0 | None ~ None ~ None
    show n_rects_ghost3_instant zorder 4                                                                                 | ST 1 | 1-0 | None ~ None ~ None
    show natsuki_ghost_blood zorder 3                                                                                    | ST 1 | 1-0 | None ~ None ~ None
#    $ style.say_dialogue = style.edited                                                                                 | ST 1 | 1-0 | None ~ None ~ None
#    n ghost2 "..."                                                                                                      | ST 1 | 2-0 | None ~ None ~ None
#    n ghost2 "PLAY WITH ME!!!"                                                                                          | ST 1 | 3-0 | None ~ None ~ None
#    $ style.say_dialogue = style.normal                                                                                 | ST 1 | 4-0 | None ~ None ~ None
    pause 5                                                                                                              | ST 1 | 5-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 5-0 | None ~ None ~ None
    # most of this section has been purely copied                                                                        | ST 1 | 0-0 | None ~ None ~ None
    # neck crack                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    play sound "sfx/crack.ogg"                                                                                           | ST 1 | 2-0 | None ~ None ~ None
    hide natsuki_ghost_blood                                                                                             | ST 1 | 2-0 | None ~ None ~ None
    hide n_rects_ghost1_instant                                                                                          | ST 1 | 2-0 | None ~ None ~ None
    hide n_rects_ghost2_instant                                                                                          | ST 1 | 2-0 | None ~ None ~ None
    hide n_rects_ghost3_instant                                                                                          | ST 1 | 2-0 | None ~ None ~ None
    show natsuki ghost3                                                                                                  | ST 1 | 2-0 | None ~ None ~ None
    show n_rects_ghost4 onlayer front zorder 4                                                                           | ST 1 | 2-0 | None ~ None ~ None
    show n_rects_ghost5 onlayer front zorder 4                                                                           | ST 1 | 2-0 | None ~ None ~ None
    pause 0.5                                                                                                            | ST 1 | 2-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 2-0 | None ~ None ~ None
    # running                                                                                                            | ST 1 | 0-0 | None ~ None ~ None
    hide natsuki                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    play sound "sfx/run.ogg"                                                                                             | ST 1 | 1-0 | None ~ None ~ None
    show natsuki ghost4 on layer front at i11                                                                            | ST 1 | 1-0 | None ~ None ~ None
    pause 0.25                                                                                                           | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    # removal                                                                                                            | ST 1 | 0-0 | None ~ None ~ None
    window hide(None)                                                                                                    | ST 1 | 1-0 | None ~ None ~ None
    hide natsuki onlayer front                                                                                           | ST 1 | 1-0 | None ~ None ~ None
    hide n_rects_ghost4 onlayer front                                                                                    | ST 1 | 1-0 | None ~ None ~ None
    hide n_rects_ghost5 onlayer front                                                                                    | ST 1 | 1-0 | None ~ None ~ None
    scene black                                                                                                          | ST 1 | 1-0 | None ~ None ~ None
    with None                                                                                                            | ST 1 | 1-0 | None ~ None ~ None
    window auto                                                                                                          | ST 1 | 1-0 | None ~ None ~ None
    scene black                                                                                                          | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    # cleanup                                                                                                            | ST 1 | 0-0 | None ~ None ~ None
    python:                                                                                                              | ST 1 | 1-0 | None ~ None ~ None
        HKBShowButtons()                                                                                                 | ST 1 | 1-0 | None ~ None ~ None
        enable_esc()                                                                                                     | ST 1 | 1-0 | None ~ None ~ None
        store.songs.enabled = True                                                                                       | ST 1 | 1-0 | None ~ None ~ None
        quick_menu = True                                                                                                | ST 1 | 1-0 | None ~ None ~ None
        renpy.music.set_volume(curr_vol, channel="music")                                                                | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 0-0 | None ~ None ~ None
    # we want to be back in the loop                                                                                     | ST 1 | 0-0 | None ~ None ~ None
    if playing_okayev:                                                                                                   | ST 1 | 1-0 | None ~ None ~ None
        $ currentpos = get_pos(channel="music")                                                                          | ST 1 | 1-0 | None ~ None ~ None
        $ adjusted_okayev = "<from " + str(currentpos) + " loop 4.444>bgm/5_monika.ogg"                                  | ST 1 | 1-0 | None ~ None ~ None
        stop music fadeout 2.0                                                                                           | ST 1 | 1-0 | None ~ None ~ None
        $ renpy.music.play(adjusted_okayev, fadein=2.0, tight=True)                                                      | ST 1 | 1-0 | None ~ None ~ None
    else:                                                                                                                | ST 1 | 1-0 | None ~ None ~ None
        stop music                                                                                                       | ST 1 | 1-0 | None ~ None ~ None
        $ play_song(store.songs.current_track)                                                                           | ST 1 | 1-0 | None ~ None ~ None
                                                                                                                         | ST 1 | 1-0 | None ~ None ~ None
    return                                                                                                               | ST 1 | 0-0 | None ~ None ~ None
